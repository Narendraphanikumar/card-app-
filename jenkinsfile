pipeline{
    agent any
    environment{
    PATH ="/home/ubuntu:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'narendra', url: 'https://github.com/Narendraphanikumar/card-app-'
            }
        }    
        stage('maven build'){
            steps{
                
                sh "mvn clean package"
            }
        }
     }
 }
 
     
     
                
            
    
       
       
